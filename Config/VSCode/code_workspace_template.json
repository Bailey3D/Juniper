{
    "folders": [
    ],
    "settings": {
        "python.pythonPath": "Y:\\Development\\Game\\tools\\Juniper\\bin\\common\\Python 3.7\\python.exe",
        "python.linting.enabled": true,
        "python.linting.flake8Enabled": true,
        "python.analysis.extraPaths": [
        ],
        "python.linting.flake8Args": [
            "--ignore=W503",
            "--ignore=E501",
            "--ignore=E402",
            "--ignore=F811",
            "--max-line-length=140",
            "--max-doc-length=160"
        ],
        "python.analysis.diagnosticSeverityOverrides": {
            "reportMissingImports": "information"
        },
        "python.analysis.autoSearchPaths": true,
        "restructuredtext.confPath": "${workspaceFolder}\\docs",
    
        "python.languageServer": "Pylance",
    
        "files.associations": {
            "**/*.local": "json"
        },
    
        "files.exclude": {
            "**/__pycache__": true,
            "**/.empty": true,
            "**/**.pycache": true,
            "**/**.pyc": true,
            "**/.vscode":true
        },
    
        "todo-tree.filtering.excludeGlobs": [
            "**/node_modules",
            "**/binaries/Python37/**"
        ],
        "search.useIgnoreFiles": false,
    
        "search.exclude":{
            "**/node_modules": true,
            "**/bower_components": true,
            "**/*.code-search": true,
            "**/Cached/**": true,
            "**/cached/**": true,
            "**/Binaries/Python/**":true
        }
    },
    "tasks": [
        {
            "label": "Run_In_3DSMax",
            "type": "shell",
            "command": "binaries/MXSPyCom.exe",
            "args": [
                "-f",
                "${file}"
            ],
            "presentation": {
                "reveal": "never",
                "panel": "new",
                "clear": true
            },
            "options": {
                "statusbar": {
                    "label": "ts"
                },
                "problemMatcher": []
            },
            "group": {
                "kind": "build",
                "isDefault": true
            }
        }
    ]
}